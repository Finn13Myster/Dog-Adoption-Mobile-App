<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFECA7"
    android:backgroundTint="#FFECA7"
    tools:context=".AsAdminViewForUpdateForm"
    tools:layout_editor_absoluteX="17dp"
    tools:layout_editor_absoluteY="-40dp">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="145dp"
        android:layout_height="151dp"
        android:layout_marginTop="594dp"
        android:layout_marginEnd="279dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pngtree_corner_border_gold_png_image_6352668__1__removebg_preview" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="149dp"
        android:layout_height="151dp"
        android:layout_marginStart="273dp"
        android:layout_marginTop="594dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/corner" />

    <EditText
        android:id="@+id/formDogName"
        android:layout_width="305dp"
        android:layout_height="54dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="85dp"
        android:ems="10"
        android:fontFamily="casual"
        android:hint="Dog Name"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/formDogImageLink"
        android:layout_width="302dp"
        android:layout_height="54dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="165dp"
        android:ems="10"
        android:fontFamily="casual"
        android:hint="Dog Image Link"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/formDogAdoptionStatus"
        android:layout_width="301dp"
        android:layout_height="56dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="245dp"
        android:ems="10"
        android:fontFamily="casual"
        android:hint="Dog Adoption Status"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/formDogAdoptedBy"
        android:layout_width="298dp"
        android:layout_height="60dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="325dp"
        android:ems="10"
        android:fontFamily="casual"
        android:hint="Dog Adopted by"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/formUpdateButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginTop="464dp"
        android:backgroundTint="#FFD362"
        android:fontFamily="casual"
        android:text="Update Dog Information"
        android:textColor="#454545"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="116dp"
        android:layout_height="90dp"
        android:layout_marginStart="148dp"
        android:layout_marginTop="396dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/yorkie_png___etsy_removebg_preview" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="161dp"
        android:layout_height="174dp"
        android:layout_marginStart="124dp"
        android:layout_marginTop="512dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/shiba_inu_removebg_preview" />

</androidx.constraintlayout.widget.ConstraintLayout>